{
  "name": "de-boissieu.fr",
  "version": "1.0.0-SNAPSHOT",
  "description": "Page perso de-boissieu.fr'",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tibidibtibo/page-perso-vue-js"
  },
  "scripts": {
    "start": "npx webpack-dev-server --inline --history-api-fallback --progress --config webpack.dev.js",
    "build": "npx webpack --config webpack.prod.js",
    "docker-build": "npx webpack --config webpack.prod.js && docker build -t tibidibtibo/de-boissieu .",
    "docker-start": "docker run -dit --name de-boissieu-dev -p 8080:80 tibidibtibo/de-boissieu",
    "docker-stop": "docker stop de-boissieu-dev",
    "docker-remove": "docker rm de-boissieu-dev && docker rmi tibidibtibo/de-boissieu",
    "docker-stop-remove": "npm run docker-stop && npm run docker-remove",
    "docker-bash": "docker exec -it de-boissieu-dev bash",
    "watch": "npm run build -- --watch",
    "test": "karma start test/unit/karma.config.js --single-run"
  },
  "author": "tibidibtibo",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.1.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "vue": "^2.4.2",
    "vue-router": "^2.7.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^1.7.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon-chai": "^1.3.2",
    "karma-webpack": "^2.0.4",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-html-webpack-plugin": "^3.2.1",
    "vue-loader": "^13.0.2",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.9.2",
    "webpack-merge": "^4.1.2"
  }
}
